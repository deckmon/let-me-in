{
	"manifest_version": 2,
	"name": "Let Me In",
	"description": "Let me in!",
	"version": "1.0",
	"browser_action": {
		"default_icon": {

		},
		"default_title": "Let me in!",
		"default_popup": "letmein_popup.html"
	},
	"permissions": [
		"activeTab",
		"tabs",
		"alarms",
		"background",
		"debugger",
        "http://*/",
        "https://*/"
	],
	"background": {
		"scripts": ["letmein_state.js"]
	}
}
